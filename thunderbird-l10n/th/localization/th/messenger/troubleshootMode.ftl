    .accesskey = ท
troubleshoot-mode-continue =
    .label = ดำเนินการต่อในโหมดแก้ไขปัญหา
    .accesskey = ด
troubleshoot-mode-quit =
    .label =
        { PLATFORM() ->
            [windows] ออก
