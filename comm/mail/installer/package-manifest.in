@APPNAME@/Contents/Library/LaunchServices
#endif
@APPNAME@/Contents/PkgInfo
@APPNAME@/Contents/Library/Spotlight/thunderbird.mdimporter/
@RESPATH@/thunderbird.icns
@RESPATH@/@LPROJ_ROOT@.lproj/*
#endif

